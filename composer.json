    "calendar",
    "ui",
    "frontend",
    "intl",
    "localization",
    "multimonth"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ThomasDev-de/bs-datepicker",
  "support": {
    "issues": "https://github.com/ThomasDev-de/bs-datepicker/issues",
    "source": "https://github.com/ThomasDev-de/bs-datepicker"
  },
  "authors": [
    {
      "name": "Thomas Kirsch",
      "email": "t.kirsch@webcito.de"
    }
  ],
  "require": {
    "components/jquery": "^3",
    "twbs/bootstrap": "^5"
  }
